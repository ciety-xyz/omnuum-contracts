{
  "name": "omnuum-contracts",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/omnuum/omnuum-contracts",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "compile": "rimraf './artifacts' './cache' '.openzeppelin' './data' && npx hardhat compile",
    "test": "npm run compile && npx hardhat test --network localhost",
    "node": "npx hardhat node",
    "run:local": "npx hardhat --network localhost run ",
    "deploy:managers:local": "npx hardhat run scripts/deployments/runDeploy.js",
    "deploy:managers:rinkeby": "npx hardhat run scripts/deployments/runDeploy.js --network rinkeby",
    "deploy:nft:local": "npm run compile && npx hardhat run scripts/deployments/deployNFTBeaconProxy.js",
    "deploy:nft:rinkeby": "npm run compile && npx hardhat run scripts/deployments/deployNFTBeaconProxy.js --network rinkeby",
    "deploy:wallet:rinkeby": "npx hardhat run scripts/deployments/deployWallet.js --network rinkeby",
    "verify:wallet": "npx hardhat verify $npm_config_address --constructor-args scripts/verifications/wallet.js --network rinkeby",
    "verify:nftBeaconProxy": "npx hardhat verify $npm_config_address --constructor-args scripts/verifications/nftBeaconProxy.js",
    "verify:nftFactory": "npx hardhat verify $npm_config_address --constructor-args scripts/verifications/nftFactory.js",
    "verify:vrfManager": "npx hardhat verify $npm_config_address --constructor-args scripts/verifications/vrfManager.js",
    "verify:revealManager": "npx hardhat verify $npm_config_address --constructor-args scripts/verifications/revealManager.js",
    "itx:registerManager": "npx hardhat run scripts/interactions/caManager/registerManager.js --network rinkeby",
    "itx:removeManager": "npx hardhat run scripts/interactions/caManager/removeManager.js --network rinkeby",
    "itx:registerNFT": "npx hardhat run scripts/interactions/caManager/registerNFT.js --network rinkeby",
    "itx:addRole": "npx hardhat run scripts/interactions/caManager/roleAddition.js --network rinkeby",
    "itx:removeRole": "npx hardhat run scripts/interactions/caManager/roleRemoval.js --network rinkeby",
    "itx:setTicketSchedule": "npx hardhat run scripts/interactions/ticketManager/setEndDate.js --network rinkeby",
    "itx:setPublicSchedule": "npx hardhat run scripts/interactions/mintManager/setPublicMintSchedule.js --network rinkeby",
    "itx:mintTicket": "npx hardhat run scripts/interactions/nft/ticketMint.js --network rinkeby",
    "itx:mintPublic": "npx hardhat run scripts/interactions/nft/publicMint.js --network rinkeby",
    "itx:airdrop": "npx hardhat run scripts/interactions/mintManager/airDrop.js --network rinkeby",
    "itx:vrfRequest": "npx hardhat run scripts/interactions/revealManager/vrfRequest.js --network rinkeby",
    "itx:setUri": "npx hardhat run scripts/interactions/nft/setURI.js --network rinkeby",
    "itx:safeTransfer": "npx hardhat run scripts/interactions/nft/safeTransfer.js --network rinkeby",
    "itx:approvalRequest": "npx hardhat run scripts/interactions/wallet/approvalRequest.js --network rinkeby",
    "itx:approve": "npx hardhat run scripts/interactions/wallet/approve.js --network rinkeby",
    "itx:revoke": "npx hardhat run scripts/interactions/wallet/revoke.js --network rinkeby",
    "itx:sendEther": "npx hardhat run scripts/interactions/wallet/sendEther.js --network rinkeby",
    "itx:payment": "npx hardhat run scripts/interactions/wallet/payment.js --network rinkeby",
    "upgrade:proxy": "npx hardhat run scripts/upgrades/upgradeProxy.js --network rinkeby",
    "upgrade:beacon": "npx hardhat run scripts/upgrades/upgradeBeacon.js --network rinkeby",
    "coverage": "npx hardhat coverage --network hardhat",
    "forceImport": "npx hardhat run scripts/upgrades/forceImport.js"
  },
  "dependencies": {
    "@marpple/omnuum-digitalSigning": "npm:@marpple/omnuum-digitalsigning@^1.0.4",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/hardhat-upgrades": "^1.17.0",
    "@openzeppelin/upgrades-core": "^1.14.1",
    "chalk": "^4.1.2",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.4",
    "fxjs": "^0.21.3",
    "hardhat": "^2.9.3",
    "hardhat-abi-exporter": "^2.8.0",
    "hardhat-contract-sizer": "^2.4.0",
    "hardhat-gas-reporter": "^1.0.7",
    "inquirer": "^8.2.1",
    "keccak256": "^1.0.6",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.7.20"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.4.0",
    "@metamask/eth-sig-util": "^4.0.0",
    "@openzeppelin/contracts": "^4.5.0",
    "@openzeppelin/contracts-upgradeable": "^4.5.2",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/test-helpers": "^0.5.15",
    "chai": "^4.3.6",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "ethereum-waffle": "^3.4.0",
    "mocha": "^9.2.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "rimraf": "^3.0.2"
  }
}
